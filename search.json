---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "subtitle"    : "{{ post.subtitle | escape }}",
      "categories" : "{{ post.categories }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "description"     : "{{ post.description | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]